var loc = ee.FeatureCollection("users/rituanilkumar/north_sikkim_bound");

//---------------------------------------------------------------------//

// Load Sentinel-2 TOA reflectance data.
var im_col = ee.ImageCollection('COPERNICUS/S2')
                  .filterDate('2020-01-16', '2020-01-31');
                  // Pre-filter to get less cloudy granules.
                  //.map(maskS2clouds);

    
var im = ee.Image(im_col.filterBounds(loc).median());

Map.setCenter(88.5, 27.63, 9);

var stfccVis = {
  min: 0.0,
  max: 10000,
  bands: ['B12', 'B11', 'B2'],
};

Map.addLayer(im.clip(loc), stfccVis, 'JAN2020');
im=im.clip(loc);

var classProperty = 'snow';
var snowcover = iceJAN2020.merge(Not_iceJAN2020);
var bands = ['B1','B2','B3','B4','B5','B6','B7','B8','B11','B12'];
var training1 = im.select(bands).sampleRegions({
  collection: snowcover,
  properties: [classProperty],
  scale: 30
});

print(training1);

//------------------------------------------------------------------//

// Load Sentinel-2 TOA reflectance data.
var im_col2 = ee.ImageCollection('COPERNICUS/S2')
                  .filterDate('2020-12-16', '2020-12-31');
                  // Pre-filter to get less cloudy granules.
                  //.map(maskS2clouds);

    
var im2 = ee.Image(im_col2.filterBounds(loc).median());

Map.setCenter(88.5, 27.63, 9);

var stfccVis = {
  min: 0.0,
  max: 10000,
  bands: ['B12', 'B11', 'B2'],
};

Map.addLayer(im2.clip(loc), stfccVis, 'DEC2020');
im2=im2.clip(loc);

var classProperty = 'snow';
var snowcover1 = iceDEC2020.merge(Not_iceDEC2020);
var bands = ['B1','B2','B3','B4','B5','B6','B7','B8','B11','B12'];
var training2 = im2.select(bands).sampleRegions({
  collection: snowcover1,
  properties: [classProperty],
  scale: 30
});

print(training2);

//------------------------------------------------------------------//

// Load Sentinel-2 TOA reflectance data.
var im_col3 = ee.ImageCollection('COPERNICUS/S2')
                  .filterDate('2021-02-01', '2021-02-16');
                  // Pre-filter to get less cloudy granules.
                  //.map(maskS2clouds);

    
var im3 = ee.Image(im_col3.filterBounds(loc).median());

Map.setCenter(88.5, 27.63, 9);

var stfccVis = {
  min: 0.0,
  max: 10000,
  bands: ['B12', 'B11', 'B2'],
};

Map.addLayer(im3.clip(loc), stfccVis, 'FEB2021');
im3=im3.clip(loc);

var classProperty = 'snow';
var snowcover2 = iceFEB2021.merge(Not_iceFEB2021);
var bands = ['B1','B2','B3','B4','B5','B6','B7','B8','B11','B12'];
var training3 = im3.select(bands).sampleRegions({
  collection: snowcover2,
  properties: [classProperty],
  scale: 30
});

print(training3);

//------------------------------------------------------------------//

// Load Sentinel-2 TOA reflectance data.
var im_col4 = ee.ImageCollection('COPERNICUS/S2')
                  .filterDate('2021-11-01', '2021-11-16');
                  // Pre-filter to get less cloudy granules.
                  //.map(maskS2clouds);

    
var im4 = ee.Image(im_col4.filterBounds(loc).median());

Map.setCenter(88.5, 27.63, 9);

var stfccVis = {
  min: 0.0,
  max: 10000,
  bands: ['B12', 'B11', 'B2'],
};

Map.addLayer(im4.clip(loc), stfccVis, 'NOV2021');
im4=im4.clip(loc);

var classProperty = 'snow';
var snowcover3 = iceNOV2021.merge(Not_iceNOV2021);
var bands = ['B1','B2','B3','B4','B5','B6','B7','B8','B11','B12'];
var training4 = im4.select(bands).sampleRegions({
  collection: snowcover3,
  properties: [classProperty],
  scale: 30
});

print(training4);

var trdata = training1.merge(training2).merge(training3).merge(training4);
print(trdata);

var snowcover = iceJAN2020.merge(Not_iceJAN2020);
Export.table.toAsset({
  collection : snowcover
});