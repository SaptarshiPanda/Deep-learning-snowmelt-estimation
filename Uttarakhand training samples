//Uttarakhand region
Map.setCenter(80.43, 30.26, 7);
//---------------------------------------------------------------------//

//Timeline 1
// Load Sentinel-2 TOA reflectance data.
var im_col1 = ee.ImageCollection('COPERNICUS/S2')
                  .filterDate('2018-01-16', '2018-01-31');
                  // Pre-filter to get less cloudy granules.
                  //.map(maskS2clouds);

    
var im1 = ee.Image(im_col1.median());

var stfccVis = {
  min: 0.0,
  max: 10000,
  bands: ['B12', 'B11', 'B2'],
};
im1=im1.clip(loc);
Map.addLayer(im1, stfccVis, 'JAN2018');

var classProperty = 'snow';
var snowcover = iceJAN2018.merge(Not_iceJAN2018);
var bands = ['B1','B2','B3','B4','B5','B6','B7','B8','B11','B12'];
var training1 = im1.select(bands).sampleRegions({
  collection: snowcover,
  properties: [classProperty],
  scale: 30
});

print(training1);

//------------------------------------------------------------------//

//Timeline 2
// Load Sentinel-2 TOA reflectance data.
var im_col2 = ee.ImageCollection('COPERNICUS/S2')
                  .filterDate('2018-12-01', '2018-12-16');
                  // Pre-filter to get less cloudy granules.
                  //.map(maskS2clouds);

    
var im2 = ee.Image(im_col2.median());

var stfccVis = {
  min: 0.0,
  max: 10000,
  bands: ['B12', 'B11', 'B2'],
};
im2=im2.clip(loc);
Map.addLayer(im2, stfccVis, 'DEC2018');

var classProperty = 'snow';
var snowcover2 = iceDEC2018.merge(Not_iceDEC2018);
var bands = ['B1','B2','B3','B4','B5','B6','B7','B8','B11','B12'];
var training2 = im2.select(bands).sampleRegions({
  collection: snowcover2,
  properties: [classProperty],
  scale: 30
});

print(training2);

//------------------------------------------------------------------//

//Timeline 3
// Load Sentinel-2 TOA reflectance data.
var im_col3 = ee.ImageCollection('COPERNICUS/S2')
                  .filterDate('2019-03-16', '2019-03-31');
                  // Pre-filter to get less cloudy granules.
                  //.map(maskS2clouds);

    
var im3 = ee.Image(im_col3.median());

var stfccVis = {
  min: 0.0,
  max: 10000,
  bands: ['B12', 'B11', 'B2'],
};
im3=im3.clip(loc);
Map.addLayer(im3, stfccVis, 'MAR2019');

var classProperty = 'snow';
var snowcover3 = iceMAR2019.merge(Not_iceMAR2019);
var bands = ['B1','B2','B3','B4','B5','B6','B7','B8','B11','B12'];
var training3 = im3.select(bands).sampleRegions({
  collection: snowcover3,
  properties: [classProperty],
  scale: 30
});

print(training3);

//------------------------------------------------------------------//

//Timeline 3
// Load Sentinel-2 TOA reflectance data.
var im_col4 = ee.ImageCollection('COPERNICUS/S2')
                  .filterDate('2019-12-01', '2019-12-16');
                  // Pre-filter to get less cloudy granules.
                  //.map(maskS2clouds);

    
var im4 = ee.Image(im_col4.median());

var stfccVis = {
  min: 0.0,
  max: 10000,
  bands: ['B12', 'B11', 'B2'],
};
im4=im4.clip(loc);
Map.addLayer(im4, stfccVis, 'DEC2019');

var classProperty = 'snow';
var snowcover4 = iceMAR2019.merge(Not_iceMAR2019);
var bands = ['B1','B2','B3','B4','B5','B6','B7','B8','B11','B12'];
var training4 = im4.select(bands).sampleRegions({
  collection: snowcover4,
  properties: [classProperty],
  scale: 30
});

print(training4);

//--------------------------------------------------------------------------//

//Merging into 1 dataset
var trainingUK = training1.merge(training2).merge(training3).merge(training4);
print('Complete Uttarakhand dataset:\n\n',trainingUK);

