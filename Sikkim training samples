//Area of interest
var loc = ee.FeatureCollection("users/rituanilkumar/north_sikkim_bound");

//---------------------------------------------------------------------//

// Load Sentinel-2 TOA reflectance data.
var im_col = ee.ImageCollection('COPERNICUS/S2')
                  .filterDate('2018-12-16', '2018-12-31');
                  // Pre-filter to get less cloudy granules.
                  //.map(maskS2clouds);

    
var im = ee.Image(im_col.filterBounds(loc).median());

Map.setCenter(88.5, 27.63, 9);

var stfccVis = {
  min: 0.0,
  max: 10000,
  bands: ['B12', 'B11', 'B2'],
};

Map.addLayer(im.clip(loc), stfccVis, 'DEC2018');
im=im.clip(loc);

var classProperty = 'snow';
var snowcover = iceDEC2018.merge(Not_iceDEC2018);
var bands = ['B1','B2','B3','B4','B5','B6','B7','B8','B11','B12'];
var training1 = im.select(bands).sampleRegions({
  collection: snowcover,
  properties: [classProperty],
  scale: 30
});

print(training1);

//------------------------------------------------------------------//

//Trial timeline 2

// Load Sentinel-2 TOA reflectance data.
var im_col_2 = ee.ImageCollection('COPERNICUS/S2')
                  .filterDate('2019-01-16', '2019-01-31');
                  // Pre-filter to get less cloudy granules.
                  //.map(maskS2clouds);

    
var im_2 = ee.Image(im_col_2.filterBounds(loc).median());

var stfccVis = {
  min: 0.0,
  max: 10000,
  bands: ['B12', 'B11', 'B2'],
};

Map.addLayer(im_2.clip(loc), stfccVis, 'JAN2019');
im_2 = im_2.clip(loc);

var classProperty = 'snow';
var snowcover2 = iceJAN2019.merge(Not_iceJAN2019);
var bands = ['B1','B2','B3','B4','B5','B6','B7','B8','B11','B12'];
var training2 = im_2.select(bands).sampleRegions({
  collection: snowcover2,
  properties: [classProperty],
  scale: 30
});

print(training2);

//------------------------------------------------------------------//

//Trial timeline 3

// Load Sentinel-2 TOA reflectance data.
var im_col_3 = ee.ImageCollection('COPERNICUS/S2')
                  .filterDate('2019-03-01', '2019-03-16');
                  // Pre-filter to get less cloudy granules.
                  //.map(maskS2clouds);

    
var im_3 = ee.Image(im_col_3.filterBounds(loc).median());

var stfccVis = {
  min: 0.0,
  max: 10000,
  bands: ['B12', 'B11', 'B2'],
};

Map.addLayer(im_3.clip(loc), stfccVis, 'MAR2019');
im_3 = im_3.clip(loc);

var classProperty = 'snow';
var snowcover3 = iceMAR2019.merge(Not_iceMAR2019);
var bands = ['B1','B2','B3','B4','B5','B6','B7','B8','B11','B12'];
var training3 = im_3.select(bands).sampleRegions({
  collection: snowcover3,
  properties: [classProperty],
  scale: 30
});

print(training3);

//-------------------------------------------------------------------//

//Trial timeline 4

// Load Sentinel-2 TOA reflectance data.
var im_col_4 = ee.ImageCollection('COPERNICUS/S2')
                  .filterDate('2019-01-16', '2019-01-31');
                  // Pre-filter to get less cloudy granules.
                  //.map(maskS2clouds);

    
var im_4 = ee.Image(im_col_4.filterBounds(loc).median());

var stfccVis = {
  min: 0.0,
  max: 10000,
  bands: ['B12', 'B11', 'B2'],
};

Map.addLayer(im_4.clip(loc), stfccVis, 'JAN2018');
im_4 = im_4.clip(loc);

var classProperty = 'snow';
var snowcover4 = iceJAN2018.merge(Not_iceJAN2018);
var bands = ['B1','B2','B3','B4','B5','B6','B7','B8','B11','B12'];
var training4 = im_4.select(bands).sampleRegions({
  collection: snowcover4,
  properties: [classProperty],
  scale: 30
});

print(training4);

var training = training1.merge(training2).merge(training3).merge(training4);
print(training);

